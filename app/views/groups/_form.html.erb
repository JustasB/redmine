<%= error_messages_for @group %>

<fieldset class="box tabular">
  <div class="control-group"><%= f.text_field :name, :required => true, :size => 60 %></div>
  <% @group.custom_field_values.each do |value| %>
    <div class="control-group"><%= custom_field_tag_with_label :group, value %></div>
  <% end %>
</fieldset>
