<div class="row pull-right lead sidetitle " id="helpTitle"><%= customiseTitle(@path) %> <i class="icon-question-sign"></i></div>

<%= javascript_include_tag('Detector.js') %>

<div class="row" id="docContainer">

	<div class="span3 bs-docs-sidebar">
		<ul id="project_overview_list" class="nav nav-list bs-docs-sidenav">
		</ul>
	</div>
  
  
	<div id="project_overview_sections" class="span9">
		<% @filesDoc.each do |fileDoc| %>
			<section id="<%= getDivId(fileDoc) %>">
				<div class="page-header">
					<h2><%= customiseTitle(fileDoc, true) %></h2>
				</div>
				<%= textilizable(open(fileDoc).read) %>
			</section>
		<% end %>
	</div>
</div>

